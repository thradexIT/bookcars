{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "strict": true,
    "jsx": "react-native",
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "rootDir": "./",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./*"
      ],
      ":bookcars-types": [
        "../packages/bookcars-types"
      ],
      ":bookcars-helper": [
        "../packages/bookcars-helper"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "references": [
    {
      "path": "../packages/bookcars-types"
    },
    {
      "path": "../packages/bookcars-helper"
    }
  ]
}
